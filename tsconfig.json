{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "declaration": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "generators",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*", "./typings/*"]
    }
  },
  "include": ["src/**/*", "./typings/*"]
}
